                    <p>26th and 27th Jan 2023</p>
                    <a href="registration" class="hero-btn">Register Now</a>

                </div>
            </div>
        </section>

        <!-- committees card -->

        <section class="mid-sec">
            <div class="container">
                <h2 class="committees-heading head-layout">Committees</h2>
                <div class="container card-section">

                    <!-- first committees disec-->

